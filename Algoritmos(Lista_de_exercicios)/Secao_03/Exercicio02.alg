algoritmo "Exercicio02"

var
num1, num2, soma, multiplicacao: inteiro

inicio
      Escreva("Digite o primeiro numero: ")
      leia(num1)
      Escreva("Digite o segundo numero: ")
      leia(num2)
      soma <- num1 + num2
      multiplicacao <- soma * num1
      Escreval("O resultado da multiplicação é: ", multiplicacao)
fimalgoritmo
